    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-subclassable-object-merger.git"
  },
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-subclassable-object-merger/issues"
  },
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "homepage": "https://github.com/do-/node-subclassable-object-merger#readme"
}
